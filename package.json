{
  "name": "for_algorithm",
  "version": "1.0.0",
  "description": "AOJとかAtCoderあたりの練習とかですかね",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "change": "sed -e s/\\\\.ts/\\\\.js/ tmpfileofnpm",
    "save": "echo > tmpfileofnpm",
    "remove": "rm tmpfileofnpm.js",
    "z": "run-s \"save -- {1}\" \"change\" node \"remove\" --",
    "x": "run-s \"tsc_out_other_file_name -- {1}\" \"node -- {2}\"  \"remove\" --",
    "tsc_out_other_file_name": "tsc --out tmpfileofnpm.js --module amd",
    "node": "node tmpfileofnpm.js < "
  },
